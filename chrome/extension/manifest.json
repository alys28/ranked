{
  "manifest_version": 3,
  "name": "Runked",
  "description": "Runked is a personalized search engine extension.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "http://localhost:5000/"  
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://localhost:5000/*" 
  ]
}
